<Window x:Class="SplitWise.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SplitWise"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Button 
                Margin="0 10 0 10"
                Background="LightCyan"
                Foreground="Black"
                x:Name="takeTSVfileButton"
                Content="Import TSV File and Calculate Settlement"
                Click="takeTSVfileButton_Click"></Button>
        <TextBox x:Name="txtBox"
                 />
        <ListView x:Name="resultListView">
            
        </ListView>
    </StackPanel>
</Window>
